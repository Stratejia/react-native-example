{
  "name": "react-native-example",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "deploy:all:dev": "eas build --platform all --profile development",
    "deploy:all:preview": "eas build --platform all --profile preview",
    "deploy:all:prod": "eas build --platform all --profile production",
    "deploy:android:dev": "eas build --platform android --profile development",
    "deploy:android:preview": "eas build --platform android --profile preview",
    "deploy:android:prod": "eas build --platform android --profile production",
    "deploy:ios:dev": "eas build --platform ios --profile development",
    "deploy:ios:preview": "eas build --platform ios --profile preview",
    "deploy:ios:prod": "eas build --platform ios --profile production",
    "eject": "expo eject",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "lint-staged",
    "lint:styles": "stylelint src/**/*.{ts,tsx}",
    "lint:styles:fix": "stylelint src/**/*.{ts,tsx} --fix",
    "prettier": "prettier .",
    "prettier:write": "prettier . --write",
    "run:android": "expo run:android",
    "run:ios": "expo run:ios",
    "start": "expo start --dev-client",
    "start:web": "expo start --web",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watchAll --passWithNoTests"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.0",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "axios": "^0.26.1",
    "dayjs": "^1.11.0",
    "expo": "~44.0.0",
    "expo-asset": "~8.4.4",
    "expo-constants": "~13.0.0",
    "expo-font": "~10.0.4",
    "expo-linking": "~3.0.0",
    "expo-splash-screen": "~0.14.0",
    "expo-status-bar": "~1.2.0",
    "expo-updates": "~0.11.7",
    "expo-web-browser": "~10.1.0",
    "i18next": "^21.6.14",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-i18next": "^11.16.2",
    "react-native": "0.64.3",
    "react-native-dotenv": "^3.3.1",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-web": "0.17.1",
    "react-query": "^3.34.19",
    "styled-components": "^5.3.5",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@react-native-community/eslint-config": "^3.0.1",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/axios": "^0.14.0",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/styled-components": "^5.1.24",
    "@types/styled-components-react-native": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-functional": "^4.2.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.7.0",
    "eslint-plugin-prefer-type-alias": "^0.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "jest-expo": "~44.0.1",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.2",
    "react-test-renderer": "17.0.1",
    "stylelint": "^14.9.1",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "~4.3.5"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": [
      "eslint . --fix",
      "prettier . --write",
      "stylelint src/**/*.{ts,tsx} --fix"
    ],
    "*.{json,md,html,js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  }
}
